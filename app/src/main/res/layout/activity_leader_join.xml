                    android:layout_height="50dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    app:boxStrokeWidthFocused="0dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/father"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="F Name"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@android:color/transparent"
                        android:textColor="@color/black_color"
                        android:textColorHint="@color/hint" />

                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Mother Name"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/mother"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="M Name"
                        android:inputType="text"
                        android:padding="10dp"
                        android:background="@android:color/transparent"
                        android:textColor="@color/black_color"
                        android:textColorHint="@color/hint" />

                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/email_layout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Country"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginEnd="30dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">


                    <Spinner
                        android:textColor="@android:color/black"
                        android:id="@+id/sp_country"
                        android:background="@drawable/bg_spinner"
                        android:layout_width="fill_parent"
                        android:layout_height="40dp"
                        />


                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/divTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/country_layout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Division"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/div_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/divTV"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginEnd="30dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <Spinner
                        android:textColor="@android:color/black"
                        android:id="@+id/sp_division"
                        android:background="@drawable/bg_spinner"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        />


                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/districtTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/div_layout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="District"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/district_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/districtTV"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <Spinner
                        android:textColor="@android:color/black"
                        android:id="@+id/sp_district"
                        android:background="@drawable/bg_spinner"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        />


                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/thanaTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/district_layout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Thana"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/thana_layout"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/thanaTV"
                    android:layout_marginStart="30dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <Spinner
                        android:textColor="@android:color/black"
                        android:id="@+id/sp_thana"
                        android:background="@drawable/bg_spinner"
                        android:layout_width="fill_parent"
                        android:layout_height="50dp"
                        />


                </androidx.cardview.widget.CardView>
                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/passoneTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/thana_layout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Password"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>
                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_below="@+id/passoneTV"
                    android:layout_marginStart="@dimen/dimen30"
                    android:background="@drawable/gradiant_white"
                    android:id="@+id/passlayout"
                    android:layout_marginEnd="@dimen/dimen30"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/ETpass"
                        android:layout_width="0dp"
                        android:layout_weight=".9"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:background="@android:color/transparent"
                        android:drawableLeft="@drawable/ic_baseline_lock_24"
                        android:drawablePadding="20dp"
                        android:drawableTint="@color/black"
                        android:fontFamily="@font/popin_regular"
                        android:hint="Password"
                        android:inputType="textPassword"
                        android:padding="8dp"
                        android:textColor="@color/black"
                        android:textColorHint="@color/hint" />

                    <ImageView
                        android:id="@+id/showPassword"
                        android:layout_gravity="center"
                        android:padding="@dimen/dimen5"
                        android:layout_width="0dp"
                        android:layout_weight=".1"
                        android:layout_height="@dimen/dimen40"
                        android:src="@drawable/eye"
                        app:tint="@color/black"/>
                </LinearLayout>


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/confirm_passTV"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/passlayout"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Confirm Password"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>



                <LinearLayout
                    android:layout_width="match_parent"
                    android:layout_below="@+id/confirm_passTV"
                    android:layout_marginStart="@dimen/dimen30"
                    android:id="@+id/cpasslayout"
                    android:background="@drawable/gradiant_white"
                    android:layout_marginEnd="@dimen/dimen30"
                    android:layout_height="wrap_content"
                    android:orientation="horizontal">


                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/ETconfirmpass"
                        android:layout_width="0dp"
                        android:layout_weight=".9"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:background="@android:color/transparent"
                        android:drawableLeft="@drawable/ic_baseline_lock_24"
                        android:drawablePadding="20dp"
                        android:drawableTint="@color/black"
                        android:fontFamily="@font/popin_regular"
                        android:hint=" Confirm Password"
                        android:inputType="textPassword"
                        android:padding="8dp"
                        android:textColor="@color/black"
                        android:textColorHint="@color/hint" />

                    <ImageView
                        android:id="@+id/showPassword2"
                        android:layout_gravity="center"
                        android:padding="@dimen/dimen5"
                        android:layout_width="0dp"
                        android:layout_weight=".1"
                        android:layout_height="@dimen/dimen40"
                        android:src="@drawable/eye"
                        app:tint="@color/black"/>
                </LinearLayout>

<!--                <androidx.appcompat.widget.AppCompatTextView-->
