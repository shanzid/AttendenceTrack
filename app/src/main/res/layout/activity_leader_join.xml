<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/bg_color"
    >

    <RelativeLayout
        android:id="@+id/one"
        android:layout_width="match_parent"
        android:background="@color/toolbar_color"
        android:layout_height="60dp">


        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/back"
            android:layout_width="50dp"
            android:layout_height="50dp"
            android:padding="10dp"
            android:tint="@color/white"
            android:src="@drawable/ic_arrow_left_black_48dp">

        </androidx.appcompat.widget.AppCompatImageView>

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginTop="5dp"
            android:layout_toEndOf="@+id/back"
            android:padding="6dp"
            android:text="@string/create_leader"
            android:textColor="@color/white"
            android:textSize="20sp"

            >

        </androidx.appcompat.widget.AppCompatTextView>

    </RelativeLayout>

    <View
        android:id="@+id/view"
        android:layout_width="match_parent"
        android:layout_height="1dp"
        android:layout_below="@+id/one"
        android:background="#E6E5E5">

    </View>

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/view">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content">


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/company_name"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginStart="30dp"
                android:layout_marginTop="15dp"
                android:text="Name"
                android:textColor="@color/black_color"
                android:textSize="18sp">

            </androidx.appcompat.widget.AppCompatTextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/company_name_layout"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/company_name"
                android:layout_marginStart="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="30dp"
                app:boxStrokeWidth="2dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp"
                app:boxStrokeWidthFocused="0dp"
                app:cardBackgroundColor="@color/white"
                app:errorEnabled="true">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/name"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="Name"
                    android:padding="10dp"
                    android:background="@android:color/transparent"
                    android:inputType="text"
                    android:textColor="@color/black_color"
                    android:textColorHint="@color/hint" />

            </androidx.cardview.widget.CardView>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/no1TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/company_name_layout"
                android:layout_marginStart="30dp"
                android:layout_marginTop="15dp"
                android:text="Mobile"
                android:textColor="@color/black_color"
                android:textSize="18sp">

            </androidx.appcompat.widget.AppCompatTextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/mob_no_layout"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/no1TV"
                android:layout_marginStart="30dp"
                android:layout_marginTop="5dp"
                app:boxStrokeWidth="2dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp"
                android:layout_marginEnd="30dp"
                app:cardBackgroundColor="@color/white"
                app:errorEnabled="true">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/number"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="+880"
                    android:inputType="text"
                    android:padding="10dp"
                    android:background="@android:color/transparent"
                    android:textColor="@color/black_color"
                    android:textColorHint="@color/hint" />

            </androidx.cardview.widget.CardView>

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/no2TV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/mob_no_layout"
                android:layout_marginStart="30dp"
                android:layout_marginTop="15dp"
                android:text="Email"
                android:textColor="@color/black_color"
                android:textSize="18sp">

            </androidx.appcompat.widget.AppCompatTextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/mob_no_2"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/no2TV"
                android:layout_marginStart="30dp"
                app:boxStrokeWidth="2dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="30dp"
                app:cardBackgroundColor="@color/white"
                app:errorEnabled="true">

                <com.google.android.material.textfield.TextInputEditText
                    android:id="@+id/email"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:hint="test@gmail.com"
                    android:inputType="text"
                    android:textColor="@color/black_color"
                    android:padding="10dp"
                    android:background="@android:color/transparent"
                    android:textColorHint="@color/hint" />

            </androidx.cardview.widget.CardView>



            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/countryTV"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/mob_no_2"
                android:layout_marginStart="30dp"
                android:layout_marginTop="15dp"
                android:text="Type"
                android:textColor="@color/black_color"
                android:textSize="18sp">

            </androidx.appcompat.widget.AppCompatTextView>

            <androidx.cardview.widget.CardView
                android:id="@+id/country_layout"
                android:layout_width="match_parent"
                android:layout_height="50dp"
                android:layout_below="@+id/countryTV"
                android:layout_marginStart="30dp"
                android:layout_marginTop="5dp"
                android:layout_marginEnd="30dp"
                app:boxStrokeWidth="2dp"
                app:cardElevation="2dp"
                app:cardCornerRadius="10dp"
                app:cardBackgroundColor="@color/white"
                app:errorEnabled="true">


                <Spinner
                    android:textColor="@android:color/black"
                    android:id="@+id/sp_type"
                    android:background="@drawable/bg_spinner"
                    android:layout_width="fill_parent"
                    android:layout_height="50dp"
                    />


            </androidx.cardview.widget.CardView>



            <LinearLayout
                android:visibility="gone"
                android:layout_below="@+id/country_layout"
                android:id="@+id/lytLeader"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">
                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="Leader"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_below="@+id/divTV"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:boxStrokeWidth="2dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <Spinner
                        android:id="@+id/sp_leader_list"
                        android:layout_width="fill_parent"
                        android:background="@drawable/bg_spinner"
                        android:layout_height="50dp"
                        />


                </androidx.cardview.widget.CardView>
            </LinearLayout>


            <LinearLayout
                android:id="@+id/root2"
                android:layout_below="@+id/lytLeader"
                android:layout_width="match_parent"
                android:orientation="vertical"
                android:layout_height="wrap_content">


                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="15dp"
                    android:text="NID"
                    android:textColor="@color/black_color"
                    android:textSize="18sp">

                </androidx.appcompat.widget.AppCompatTextView>

                <androidx.cardview.widget.CardView
                    android:id="@+id/company_nid"
                    android:layout_width="match_parent"
                    android:layout_height="50dp"
                    android:layout_marginStart="30dp"
                    android:layout_marginTop="5dp"
                    android:layout_marginEnd="30dp"
                    app:cardElevation="2dp"
                    app:cardCornerRadius="10dp"
                    app:boxStrokeWidthFocused="0dp"
                    app:cardBackgroundColor="@color/white"
                    app:errorEnabled="true">

                    <com.google.android.material.textfield.TextInputEditText
                        android:id="@+id/nid"
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:hint="nid"
                        android:padding="10dp"
                        android:background="@android:color/transparent"
                        android:inputType="text"
                        android:textColor="@color/black_color"
                        android:textColorHint="@color/hint" />

                </androidx.cardview.widget.CardView>

                <androidx.appcompat.widget.AppCompatTextView
